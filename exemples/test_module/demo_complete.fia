# === COMBINAISON DE PLUSIEURS MODULES ===
imprimer("\nüéØ === EXEMPLE AVANC√â ===")
soit donnees = [12.5, 18.3, 9.7, 15.2, 21.1]

fonction analyser_donnees(valeurs) {
    soit somme = 0
    soit minimum = valeurs[0]
    soit maximum = valeurs[0]
    
    pour val dans valeurs {
        somme += val
        si (val < minimum) {
            minimum = val
        }
        si (val > maximum) {
            maximum = val
        }
    }
    
    soit moyenne = somme / longueur(valeurs)
    
    retourner {
        "moyenne": moyenne,
        "minimum": minimum,
        "maximum": maximum,
        "nombre": longueur(valeurs)
    }
}

soit analyse = analyser_donnees(donnees)
imprimer("Donn√©es:", donnees)
imprimer("Analyse compl√®te:")
imprimer("  ‚Ä¢ Moyenne:", math.arrondir(analyse["moyenne"], 2))
imprimer("  ‚Ä¢ Minimum:", analyse["minimum"])
imprimer("  ‚Ä¢ Maximum:", analyse["maximum"])
imprimer("  ‚Ä¢ Nombre de valeurs:", analyse["nombre"])

